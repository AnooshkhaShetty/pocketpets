    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/images/pets_logo.png",
      "resizeMode": "contain",
      "backgroundColor": "#D9C6C7"
    },
    "assetBundlePatterns": [
